{
  "name": "person-blog",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=@person_blog/web",
    "dev:server": "turbo run dev --filter=@person_blog/server-main",
    "dev:ui:demo": "turbo run dev --filter=@person_blog/ui",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
  },
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "vue": "3.5.18",
      "typescript": "5.9.2",
      "eslint": "9.34.0",
      "typescript-eslint": "8.42.0"
    }
  },
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "husky": "^9.1.7",
    "turbo": "^2.5.5",
    "validate-commit-msg": "^2.14.0",
    "typescript": "5.9.2",
    "eslint": "9.34.0",
    "typescript-eslint": "8.42.0",
    "vite": "^7.0.6",
    "jiti": "^2.4.2",
    "rimraf": "^6.0.1"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23"
  }
}